  },
  "dependencies": {
    "axios": "^0.18.0",
    "cli-progress": "^2.1.1",
    "colors": "^1.3.3",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "long": "^4.0.0",
    "mca-js": "^0.1.1",
    "nbt": "^0.8.1",
    "node-int64": "^0.4.0",
    "node-stream-zip": "^1.8.0",
    "nopt": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
